{
    "short_name": "BgColor Change",
    "name": "Change background color",
    "version": "1.0.0",
    "manifest_version": 2,
    "permissions": [
      "storage",
      "tabs",
      "activeTab",
      "http://*/*", 
      "https://*/*"
    ],
    "browser_action": {
      "default_popup": "extension/build/index.html",
      "default_icon": {
        "16": "/images/logo192.png",
        "32": "/images/logo192.png",
        "48": "/images/logo192.png",
        "128": "/images/logo192.png" 
      }
    },
    "icons": {
      "16": "/images/logo192.png",
      "32": "/images/logo192.png",
      "48": "/images/logo192.png",
      "128": "/images/logo192.png"
    },
    "content_security_policy": "script-src 'self' 'sha256-MnwXpRCcLkk0LAXXGTfvvQeiAnGgu86HpxQJfrDy6J8='; object-src 'self';",
    "web_accessible_resources": ["api.js"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "match_about_blank": true,
        "js": ["hello.js"]
      }
    ]
  }